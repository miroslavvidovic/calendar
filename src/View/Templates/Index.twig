{% extends "Base.twig" %}

{% block title %} Home {% endblock %}

{% block scripts %}
<script>
$(document).ready(function() {
    // page is now ready, initialize the calendar...
   $('#calendar').fullCalendar({
    header: {
      left:'prev,next today',
      center: 'title',
      right: 'month, agendaWeek, agendaDay'
    },
    events: '../Feed.php',
    editable: true
  });
});
</script>
{% endblock %}
{% block content %}
      <h2>Events</h2>
      <div id="calendar"></div>
{% endblock %}
